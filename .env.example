# Private key for EOA (Externally Owned Account)
PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Fuse API key
PUBLIC_API_KEY=pk_your_api_key_here

# ERC-20 token address for testing (default: USDC on Fuse)
TOKEN_ADDRESS=0x34Ef2Cc892a88415e9f02b91BfA9c91fC0bE6bD4

# Paymaster sponsor ID
SPONSOR_ID=your_sponsor_id_here

# Paymaster contract address
PAYMASTER_ADDRESS=0xYourPaymasterAddressHere

# RPC endpoint (must be HTTP/HTTPS, not WebSocket)
RPC_URL=https://rpc.fuse.io

# Bundler endpoint
BUNDLER_URL=https://api.fuse.io/api/v0/bundler?apiKey=YOUR_API_KEY

# Paymaster endpoint
PAYMASTER_URL=https://api.fuse.io/api/v0/paymaster?apiKey=YOUR_API_KEY

# Use Etherspot wallet (true) or SimpleAccount (false)
USE_ETHERSPOT=true

